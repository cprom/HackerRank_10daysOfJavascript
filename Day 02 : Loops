function vowelsAndConsonants(s) {
    var vowels = "";
    var consonants = "";
    for (let i = 0; i < s.length; i++) {
      
      if (s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u'){
        vowels += s[i]+'\n';
    
      } else{
        consonants += s[i]+'\n';
      }
    }
    console.log(vowels + consonants);
}
